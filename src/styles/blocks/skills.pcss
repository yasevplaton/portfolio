.skills__container {
  /* padding: 10% 0; */
}

.skills__content {
  display: flex;
  align-items: flex-end;
}

.skills__item {
  position: relative;
  width: 135px;
  height: 135px;
}

.skills__title {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: inline-block;
  text-align: center;
}

.skills__chart {
  width: 100%;
  height: 100%;
}

